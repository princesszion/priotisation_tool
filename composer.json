{
	"description": "CodeIgniter 3 framework with Eloquent ORM",
	"name": "mod-ci/with-eloquent",
	"type": "project",
	"homepage": "https://github.com/aididalam/Codeigniter-Eloquent-ORM",
	"license": "MIT",
	"authors": [
		{
			"name": "Aidid Alam",
			"email": "aididalam11@gmail.com"
		}
	],
	"support": {
		"forum": "https://github.com/aididalam/Codeigniter-Eloquent-ORM/discussions/1",
		"wiki": "https://github.com/bcit-ci/CodeIgniter/wiki",
		"slack": "https://codeigniterchat.slack.com",
		"source": "https://github.com/bcit-ci/CodeIgniter"
	},
	"require": {
		"php": ">=5.3.7",
			"illuminate/database": "^8.83",
			"symfony/var-dumper": "^5.4",
			"illuminate/pagination": "^8.83",
			"shufo/laravel-opensearch": "^0.1.4",
			"thybag/php-sharepoint-lists-api": "dev-master",
			"phpmailer/phpmailer": "^6.9",
			"react/react": "^1.4",
		"guzzlehttp/guzzle": "^7.0",
		"whtht/perfectly-cache": "^4.3",
		"firebrandhq/silverstripe-excel-export": "^0.2.2",
		"vlucas/phpdotenv": "^5.6",
		"elasticsearch/elasticsearch": "^8.17"
		
	},
	"autoload": {
		"psr-4": {
			"utils\\": "utils/"
		}
	},
	"suggest": {
		"paragonie/random_compat": "Provides better randomness in PHP 5.x"
	},
	"require-dev": {
		"phpunit/phpunit": "4.* || 5.* || 9.*"
	},
	"config": {
		"allow-plugins": {
			"composer/installers": false,
			"php-http/discovery": true
		}
	}
}
